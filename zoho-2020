import java.util.Scanner;

public class Zoho {
	public static void main(String args[]) {
		Scanner s = new Scanner(System.in);
		int num = s.nextInt();
		
		for (int i = num; i > 0; i--) 
		{ 
		    for (int j = num; j > i; j--) 
		        System.out.print(j); 
		 
		    for (int j = i; j > 0; j--) 
		        System.out.print(i); 
		 
		    System.out.println(); 
		} 
	}
}
